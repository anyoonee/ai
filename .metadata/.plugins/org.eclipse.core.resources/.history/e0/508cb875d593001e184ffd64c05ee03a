package javaBasic1204.ch04.sec02;

public class while문연습 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//246810 출력
		
		int a = 2;
		/*
		while (a<10) {
			a=a+2;
			System.out.println(a);
		}
		*/
		while(a<=10) {
			System.out.println(a);
			a=a+2;
		}
		/*
		do {
			//반복할 내용
		}while(조건식);
		*/
		
		int n2 =1;
		do {
			System.out.println(n2);
			n2++;
		}while(n2 <=10);
		
		
	}

}
