package javaBasic1204.ch04.sec02;

public class 확인문제4장 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//3 for 문 이용해 1~100 정수중 3의배수의 총합 출력
		int sum =0; // 3의배수의합을 넣을변수
		for(int a=1; a<=100; a++) {
			if( a%3 == 0) {
				sum=sum + a;
			}
				
			System.out.println("1~100까지 3의배수 합은" + sum);
		}
			
			
		//5 중첩for문 이용해 4x+5y=60 모든해 구해서 (x,y)형태로 출력 x,y는 10이하
			for(int x=1; x<=10; x++) {
				for(int y=1; y<=10; y++) {
					if(4*x + 5*y ==60) {
						System.out.println("x=" + x +"y"+ y);
					}
				}
			}
			
	}

}
